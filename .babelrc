{
  "presets": ["react", "es2015", "stage-0"],
  "plugins": [
    [
      "module-alias",
      [
        { "src": "./app", "expose": "app" }
      ],
    ],
    ["transform-runtime", {
      "helpers": false,
      "polyfill": false,
      "regenerator": true,
      "moduleName": "babel-runtime"
    }],
  ]
}
